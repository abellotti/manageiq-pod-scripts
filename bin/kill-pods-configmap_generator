if [ "$(minishift status)" != "Running" ]
then
  echo "Minishift is not running"
else
  echo "Killing httpd_configmap_generator pods and resources ..."
  oc login $(minishift ip):8443 -u developer -p developer
  oc delete secret -l app=httpd_configmap_generator
  oc delete all -l app=httpd_configmap_generator
  oc delete pods -l app=httpd_configmap_generator
fi
