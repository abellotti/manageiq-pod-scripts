if [ "$(minishift status)" != "Running" ]
then
  echo "Minishift is not running"
else
  echo "Killing manageiq-httpd-authconfig pods and resources ..."
  oc login $(minishift ip):8443 -u developer -p developer
  oc delete secret -l app=manageiq-httpd-authconfig
  oc delete all -l app=manageiq-httpd-authconfig
  oc delete secret -l app=manageiq-httpd-authconfig
  oc delete pods -l app=manageiq-httpd-authconfig
fi
